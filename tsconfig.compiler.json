{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
    "noEmitOnError": true,
    "strict": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "pretty": true,
    "outDir": "dist",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  }
}
